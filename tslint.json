{
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "no-console": false,
    "ordered-imports": false,
    "no-string-literal": false,
    "no-empty": false,
    "object-literal-sort-keys": false,
    "no-floating-promises": true,
    "trailing-comma": false,
    "interface-name": false,
    "whitespace": [
      true,
      "check-module"
    ]
  }
}
